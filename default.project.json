{
  "name": "barge-simulator",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/client"
        }
      }
    },

    "StarterGui": {
      "$ignoreUnknownInstances": true,
      "JobMenu": {
        "$className":"ScreenGui",
        "$ignoreUnknownInstances": true,
        "PickJobButton": {
          "$className":"TextButton",
          "$ignoreUnknownInstances": true,
          "LocalScript": {
            "$path": "src/startergui/JobMenu/PickJobButton/LocalScript.client.lua"
          }
        },
        "JobStatusLabel": {
          "$className":"TextLabel",
          "$ignoreUnknownInstances": true,
          "LocalScript": {
            "$path": "src/startergui/JobMenu/JobStatusLabel/LocalScript.client.lua"
          }
        },
        "JobListFrame": {
          "$className":"Frame",
          "$ignoreUnknownInstances": true,
          "JobTemplate": {
            "$className":"TextButton",
            "$ignoreUnknownInstances": true
          }
        }
      }
    },

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 2,
        "GlobalShadows": true,
        "Outlines": false,
        "Technology": "Voxel"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}